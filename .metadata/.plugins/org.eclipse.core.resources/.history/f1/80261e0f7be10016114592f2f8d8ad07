package basic;

public class Variable {
	public static void main(String[] args){
		char c1 = 'J';	
		char c2 = '中';
		char c3 = 74;	//ASCII(65)指的是J
		char[] cArray;
		char n = '\0';	//空字元

		byte b1 = 1;
		byte b2 = 'J';	//J的ASCII = 74
		byte b3 = '1';	//1的ASCII = 49
		byte b4 = 127;
		byte b5 = 052;
		byte b6 = 0x1F;
		byte b7 = 0b1110;
		byte[] bArray;
		System.out.println(b5+","+b6+","+b7);
		
		short s1 = 1;
		short s2 = 'A';
		short[] sArray;
		short s3 = 1_000;
		
		
	}
}
