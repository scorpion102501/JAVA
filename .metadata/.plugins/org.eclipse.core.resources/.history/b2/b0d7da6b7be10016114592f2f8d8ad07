package basic;

public class Variable {
	public static void main(String[] args){
		/*char*/
		char c1 = 'J';	
		char c2 = '中';
		char c3 = 74;	//ASCII(65)指的是J
		char[] cArray;
		char n = '\0';	//空字元
		/*byte*/
		byte b1 = 1;
		byte b2 = 'J';		//J的ASCII = 74
		byte b3 = '1';		//1的ASCII = 49
		byte b4 = 127;
		byte b5 = 052;		//42
		byte b6 = 0x1F;		//31
		byte b7 = 0b1110;	//14
		byte[] bArray;
		/*short*/
		short s1 = 1;
		short s2 = 'A';
		short[] sArray;
		short s3 = 1_000;
		/*int*/
		int i1 = 17;
		int[] iArray;
		int i2 = 52_000;
	}
}
